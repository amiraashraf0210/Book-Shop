new { Name = "Atomic Habits", Author = "James Clear", Image = "https://m.media-amazon.com/images/I/81wgcld4wxL._AC_UF1000,1000_QL80_.jpg", Price = 19.99m, Genre = "SelfHelp" },
new { Name = "Normal People", Author = "Sally Rooney", Image = "https://th.bing.com/th/id/OIP.zDuyB5twsQkBRIffkkVciQHaHa?w=201&h=201&c=7&r=0&o=5&dpr=1.3&pid=1.7", Price = 13.99m, Genre = "Romance" },
new { Name = "Conversations with Friends", Author = "Sally Rooney", Image = "https://th.bing.com/th/id/OIP._mye4adU9GERAFIlZOFW4gHaLY?w=121&h=186&c=7&r=0&o=5&dpr=1.3&pid=1.7", Price = 12.99m, Genre = "Romance" },
new { Name = "Project Hail Mary", Author = "Andy Weir", Image = "https://th.bing.com/th/id/OIP.gdk85yXCBt0drdnBKSU_gQHaHa?w=212&h=212&c=7&r=0&o=5&dpr=1.3&pid=1.7", Price = 17.99m, Genre = "SciFi" }, 
new { Name = "The Martian", Author = "Andy Weir", Image = "https://th.bing.com/th/id/OIP.arNGPFXScldBIGqOaBdfYQHaLb?w=115&h=180&c=7&r=0&o=5&dpr=1.3&pid=1.7", Price = 14.99m, Genre = "SciFi" }, 
new { Name = "To Kill a Mockingbird", Author = "Harper Lee", Image = "https://th.bing.com/th/id/OIP.T55C4BiOVfcS3WFdCM3xdwHaK2?w=206&h=302&c=7&r=0&o=5&dpr=1.3&pid=1.7", Price = 11.99m, Genre = "Crime" },
    new { Name = "Jack Reacher: Killing Floor", Author = "Lee Child", Image = "https://th.bing.com/th/id/ODL.0eb69c2eb5676cc538eeb3ed94f09dbb?w=195&h=300&c=10&rs=1&o=6&dpr=1.3&pid=AlgoBlockDebug", Price = 14.99m, Genre = "Action" },

};

    var filteredBooks = allBooks;

    // Filter by search term
    if (!string.IsNullOrEmpty(Model.STerm))
    {
        filteredBooks = filteredBooks.Where(b =>
        b.Name.ToLower().Contains(Model.STerm.ToLower()) ||
        b.Author.ToLower().Contains(Model.STerm.ToLower()) ||
        b.Genre.ToLower().Contains(Model.STerm.ToLower())
        ).ToArray();
    }

    // Filter by genre
    if (Model.GenreId > 0)
    {
        var selectedGenre = Model.Genres.FirstOrDefault(g => g.Id == Model.GenreId)?.GenreName;
        if (!string.IsNullOrEmpty(selectedGenre))
        {
            filteredBooks = filteredBooks.Where(b => b.Genre.Equals(selectedGenre, StringComparison.OrdinalIgnoreCase)).ToArray();
        }
    }
}

<div class="container">
    <!-- Search and Filter Section -->
    <div class="search-section bg-white p-4 rounded-3 shadow-sm mb-4">
        <form asp-action="Index" method="get" class="row g-3">
            <div class="col-md-4">
                <label class="form-label">Genre</label>
                <select class="form-select" id="genreId" name="genreId" onchange="this.form.submit()">
                    <option value="">All Genres</option>
                @foreach (var genre in Model.Genres)
                {
                    <option selected="@(genre.Id == Model.GenreId)" value="@genre.Id">@genre.GenreName</option>
                }
            </select>
        </div>

            <div class="col-md-8">
                <label class="form-label">Search Books</label>
            <div class="input-group">
                    <span class="input-group-text"><i class="bi bi-search"></i></span>
                    <input type="text" class="form-control" value="@Model.STerm" id="sterm" name="sterm"
                        placeholder="Search by title, author, or genre">
                    <button type="submit" class="btn btn-primary">
                        <i class="bi bi-search"></i> Search
                    </button>
                </div>
        </div>
    </form>
</div>

    <!-- Books Display Section -->
    <div class="books-section">
        <h2 class="section-title mb-4">
            @if (!string.IsNullOrEmpty(Model.STerm))
            {
                <span>Search Results for "@Model.STerm"</span>
            }
            else if (Model.GenreId > 0)
            {
                var selectedGenre = Model.Genres.FirstOrDefault(g => g.Id == Model.GenreId)?.GenreName;
                <span>@selectedGenre Books</span>
            }
            else
            {
                <span>All Books</span>
            }
        </h2>
        <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 g-4">
            @foreach (var book in filteredBooks)
            {
                <div class="col">
                    <div class="card h-100 book-card">
                        <div class="book-image-container">
                            <img src="@book.Image" class="card-img-top" alt="@book.Name">
                        </div>
            <div class="card-body">
                            <h5 class="card-title">@book.Name</h5>
                <p class="card-text">
                                <span class="text-muted">by @book.Author</span><br>
                                <span class="badge bg-primary">@book.Genre</span>
                            </p>
                            <div class="d-flex justify-content-between align-items-center">
                                <span class="price">$@book.Price</span>
                                <button class="btn btn-primary"
                                    onclick="addToCart('@book.Name', '@book.Image', @book.Price)">
                                    <i class="bi bi-cart-plus"></i> Add to Cart
                                </button>
                            </div>
                        </div>
            </div>
        </div>
    }
</div>
    </div>
</div>

<style>
    .section-title {
        color: var(--primary-color);
        font-weight: 600;
        position: relative;
        padding-bottom: 10px;
    }

    .section-title::after {
        content: '';
        position: absolute;
        left: 0;
        bottom: 0;
        width: 50px;
        height: 3px;
        background-color: var(--secondary-color);
    }

    .book-card {
        transition: transform 0.3s ease, box-shadow 0.3s ease;
        border: none;
        background: var(--card-background);
    }

    .book-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);
    }

    .book-image-container {
        height: 300px;
        overflow: hidden;
        position: relative;
    }

    .book-image-container img {
        width: 100%;
        height: 100%;
        object-fit: cover;
        transition: transform 0.3s ease;
    }

    .book-card:hover .book-image-container img {
        transform: scale(1.05);
    }

    .card-title {
        font-size: 1.1rem;
        font-weight: 600;
        margin-bottom: 0.5rem;
        color: var(--primary-color);
    }

    .price {
        font-size: 1.2rem;
        font-weight: 600;
        color: var(--accent-color);
    }

    .search-section {
        background: linear-gradient(135deg, var(--background-color) 0%, var(--card-background) 100%);
    }

    .form-control:focus,
    .form-select:focus {
        border-color: var(--secondary-color);
        box-shadow: 0 0 0 0.2rem rgba(22, 160, 133, 0.25);
    }

    .badge {
        font-weight: 500;
        padding: 0.5em 0.8em;
        background-color: var(--secondary-color) !important;
    }

    .books-section {
        min-height: calc(100vh - 400px);
    }
</style>

@section Scripts {
    <script>
        async function addToCart(bookName, bookImage, price) {
             var usernameEl = document.getElementById("username");
            if (usernameEl == null) {
            window.location.href = "/Identity/Account/Login";
                return;
        }
            try {
                var response = await fetch(`/Cart/AddItem?bookId=1&bookName=${encodeURIComponent(bookName)}&bookImage=${encodeURIComponent(bookImage)}&price=${price}`);
                if (response.status == 200) {
                    var result = await response.json();
                    var cartCountEl = document.getElementById("cartCount");
                    cartCountEl.innerHTML = result;
                }
            }
            catch (err) {
                console.log(err);
            }
        }
    </script>
}
