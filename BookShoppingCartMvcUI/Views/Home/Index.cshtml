new { Name = "The Intelligent Investor", Author = "Benjamin Graham", Image = "https://m.media-amazon.com/images/I/91OINeHnJGL.jpg", Price = 27.99m, Genre = "SelfHelp" },
new { Name = "The Shining", Author = "Stephen King", Image = "https://th.bing.com/th/id/OIP.hPtCxMr3od5xCppqm-mqRgHaL3?w=197&h=316&c=7&r=0&o=5&dpr=1.3&pid=1.7", Price = 13.99m, Genre = "Thriller" },
new { Name = "11/22/63", Author = "Stephen King", Image = "data:image/jpeg;base64,", Price = 16.99m, Genre = "Thriller" },
new { Name = "Verity", Author = "Colleen Hoover", Image = "https://m.media-amazon.com/images/I/71QKQ9mwV7L.jpg", Price = 13.99m, Genre = "Thriller" },
new { Name = "It Starts With Us", Author = "Colleen Hoover", Image = "https://m.media-amazon.com/images/I/81vpsIs58WL.jpg", Price = 14.99m, Genre = "Romance" },
new { Name = "Where the Crawdads Sing", Author = "Delia Owens", Image = "https://m.media-amazon.com/images/I/81WojUxbbFL.jpg", Price = 16.99m, Genre = "Crime" },
