new { Name = "The Alchemist", Author = "Paulo Coelho", Image = "https://m.media-amazon.com/images/I/71aFt4+OTOL.jpg", Price = 13.99m, Genre = "SelfHelp" },
new { Name = "Brave New World", Author = "Aldous Huxley", Image = "data:image/jpeg;base64,", Price = 14.99m, Genre = "Thriller" },
new { Name = "The Subtle Art of Not Giving a F*ck", Author = "Mark Manson", Image = "https://m.media-amazon.com/images/I/71QKQ9mwV7L.jpg", Price = 15.99m, Genre = "SelfHelp" },
new { Name = "The Great Gatsby", Author = "F. Scott Fitzgerald", Image = "data:image/jpeg;base64,", Price = 9.99m, Genre = "Romance" },
new { Name = "Sapiens", Author = "Yuval Noah Harari", Image = "https://m.media-amazon.com/images/I/713jIoMO3UL.jpg", Price = 20.99m, Genre = "SelfHelp" },
